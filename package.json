  "bugs": {
    "url": "https://github.com/peruvianidol/mikeaparicio.com/issues"
  },
  "homepage": "https://github.com/peruvianidol/mikeaparicio.com#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.38.0"
  }
